public class Fornecedores {
	private Long codigo;
	private String nome;
	
	public Long getCodigo() {
		return codigo;
	}
	public void setCodigo(long codigo) {
		this.codigo = codigo;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.descricao = nome;
	}
	
	@Override
	public String toString() {
		String saida = codigo + " - " + nome;
		return saida;
	}
	
}
